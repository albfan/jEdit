# Dependencies
#
plugin.macos.MacOSPlugin.depend.0=jedit 04.00.05.00
plugin.macos.MacOSPlugin.depend.1=jdk 1.3

MacOSPlugin.depend.os.name=Mac OS X
MacOSPlugin.depend.mrj.version=3.2

