<?xml version="1.0"?>

<!DOCTYPE DOCKABLES SYSTEM "dockables.dtd">

<DOCKABLES>
	<DOCKABLE NAME="log-viewer">
		new LogViewer();
	</DOCKABLE>
	<DOCKABLE NAME="vfs.browser">
		new VFSBrowser(view,position);
	</DOCKABLE>
	<DOCKABLE NAME="hypersearch-results">
		new HyperSearchResults(view);
	</DOCKABLE>
	<DOCKABLE NAME="view-registers">
		new RegisterViewer(view,position);
	</DOCKABLE>
	<DOCKABLE NAME="view-markers">
		new MarkerViewer(view);
	</DOCKABLE>
	<DOCKABLE NAME="io-progress-monitor">
		new IOProgressMonitor();
	</DOCKABLE>
</DOCKABLES>
