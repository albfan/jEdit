<?xml version="1.0"?>

<!DOCTYPE DOCKABLES SYSTEM "dockables.dtd">

<DOCKABLES>
	<DOCKABLE NAME="log-viewer">
		new LogViewer();
	</DOCKABLE>
	<DOCKABLE NAME="vfs.browser">
		new VFSBrowser(view,null,VFSBrowser.BROWSER,false,
			position.equals(DockableWindowManager.FLOATING));
	</DOCKABLE>
	<DOCKABLE NAME="hypersearch-results">
		new HyperSearchResults(view);
	</DOCKABLE>
</DOCKABLES>
